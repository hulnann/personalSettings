   "箭头函数": {
     "prefix": "af",
     "body": [
      "$1 => {",
      "  $2",
      "}" 
     ],
     "description": "af输出箭头函数"
   },
   "单行箭头函数": {
     "prefix": "aff",
     "body": "($1) => $2",
